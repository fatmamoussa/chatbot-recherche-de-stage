{
  "name": "chatbot-recherche-stage",
  "version": "1.0.0",
  "description": "Chatbot IA + NLP pour la recherche de stage adaptative",
  "homepage": "https://github.com/fatmamoussa/chatbot-recherche-de-stage#readme",
  "bugs": {
    "url": "https://github.com/fatmamoussa/chatbot-recherche-de-stage/issues"
  },
  "repository": {
